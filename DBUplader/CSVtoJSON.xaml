<Window x:Class="DBUplader.CSVtoJSON"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DBUplader"
        mc:Ignorable="d"
        Title="CSVtoJSON" Height="300" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Name="ChooseCSV" Content="Choose CSV file" Click="ChooseCSV_Click"/>
        <TextBox Name="pathTextBox" Grid.Row="1"/>
        <Button Name="Convert" Content="Convert" Click="Convert_Click" Grid.Row="2"/>
    </Grid>
</Window>
